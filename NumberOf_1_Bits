package com.learn.practices;

public class Count1Bytes {

	public static void main(String[] args) 
	{
	    // you need to treat n as an unsigned value
		int n=00000000000000000000000000001011;
		int ones = 0;
		   while(n!=0) 
		   {
			   ones += (n & 1);
			   n>>>=1;
		    }
		   System.out.println( ones);  
		    
		}
}
